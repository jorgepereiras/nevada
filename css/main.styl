		float left
		margin-left 20px
		width 430px
		h4
			color gris
			fuente(normal,300,30px)
			margin 50px 0 20px 0
			text-transform uppercase
		p
			color gris
			fuente(normal,300,14px)
			margin 0 0 10px 10px
		h6
			color gris
			fuente(normal,300,12px)
			margin-top 20px
		.watercolor
			display inline-block
			height 44px
			margin 5px
			width 46px
		.negro
			background-color #000
		.cafe
			background-color #1b0c03
		.vino
