.listRelacionados
	background #FFF url(../img/deg-relacion.png) repeat-x
	height 120px
	ul
		li
			display inline-block
			height 120px
			margin 0 10px
			overflow hidden
			position relative
			top 0
			transition(all 0.2s ease 0.2s)
			z-index 100
			img
				display block
				opacity 0.5
				height 100px
				margin-top -5px
				transition(all 0.2s ease 0.2s)
			a
				background-color #e0e0e0
				bottom 0
				color gris
				display block
				fuente(normal,400,12px)
