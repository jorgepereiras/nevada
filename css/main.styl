				text-transform uppercase
			&:hover
				top 10px
				box-shadow(#333333 0 15px 15px)
				img
					opacity 1

#detalleProducto
	background-color #E0E0E0
	overflow hidden
	position relative
	.list-views
		left 0
		position absolute
		top 60px
		width 130px
		a
			display inline-block
			border 2px solid #CCCCCC
			height 50px
			margin 5px
			opacity 0.8
			transition(all 0.2s ease 0.2s)
			width 50px
			img
				width 100%
			&:hover
				opacity 1
	.viewProducto
		float left
		min-height 500px
		width 750px
		.view
			float right
			height 400px
			position relative
			width 500px
			img
				margin-top -5%
				width 100%
	.linea
		color gris
		fuente(normal,700,40px)
		opacity 0.2
		position absolute
		left 20px
		top 250px
		text-transform uppercase
